{
  "name": "LiveServer",
  "displayName": "GoLive",
  "description": "Launch a server directly from VS Code editor.",
  "version": "0.0.2",
  "publisher": "ritwickdey",
  "engines": {
    "vscode": "^1.13.0"
  },
  "categories": [
    "Other",
    "Extension Packs"
  ],
  "keywords": [
    "html",
    "live",
    "live preview",
    "live server",
    "live reload",
    "preview"
  ],
  "galleryBanner": {
    "color": "#04e869",
    "theme": "dark"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.liveServer.goOffline",
        "title": "Go Offline Server"
      },
      {
        "command": "extension.liveServer.goOnline",
        "title": "Go Online Server"
      }
    ]
  },
  "license": "MIT",
  "icon": "images/icon.png",
  "bugs": {
    "url": "https://github.com/ritwickdey/LiveServer-VScodeExtension/issues",
    "email": "ritwickdey@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ritwickdey/LiveServer-VScodeExtension.git"
  },
  "homepage": "https://github.com/ritwickdey/LiveServer-VScodeExtension/blob/master/README.md",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "live-server": "^1.2.0"
  }
}
{
    "name": "LiveServer",
    "displayName": "Live Server",
    "description": "Launch Local Server to browser directly from VSCode Editor by a single click.",
    "version": "1.0.0",
    "publisher": "ritwickdey",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other",
        "Extension Packs"
    ],
    "keywords": [
        "live",
        "live preview",
        "live server",
        "live reload"
    ],
    "preview": true,
    "galleryBanner": {
        "color": "#41205f",
        "theme": "dark"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.liveServer.goOffline",
                "title": "Go Offline Server"
            },
            {
                "command": "extension.liveServer.goOnline",
                "title": "Go Live Server"
            }
        ]
    },
    "license": "MIT",
    "icon": "images/icon.png",
    "bugs": {
        "url": "https://github.com/ritwickdey/LiveServer-VScodeExtension/issues",
        "email": "ritwickdey@outlook.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ritwickdey/LiveServer-VScodeExtension.git"
    },
    "homepage": "https://github.com/ritwickdey/LiveServer-VScodeExtension/blob/master/README.md",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "http-shutdown": "^1.2.0",
        "live-server": "^1.2.0"
    }
}